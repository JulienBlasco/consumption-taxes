use "E:\Code\21-03 Datasets V7 (JPubEc Resubmit)\DTA\ConsumptionTaxes_percentiles_xtnddmodel.dta", clearkeep if ccyy == "us13"gen group = "P1-20" if percentile <= 20replace group = "P21-40" if percentile > 20 & percentile <= 40replace group = "P41-60" if percentile > 40 & percentile <= 60replace group = "P61-80" if percentile > 60 & percentile <= 80replace group = "P81-95" if percentile > 80 & percentile <= 95replace group = "P96-99" if percentile > 95 & percentile <= 99replace group = "P99,9-100" if percentile == 100egen tax_pred_group = mean(tax_pred), by(group)egen dhi_group = mean(dhi), by(group)gen taxrate_group = tax_pred_group/dhi_groupgen taxrate_group_ITEP = 0.071202 if group == "P1-20"replace taxrate_group_ITEP = 0.059358 if group == "P21-40"replace taxrate_group_ITEP = 0.047539 if group == "P41-60"replace taxrate_group_ITEP = 0.037639 if group == "P61-80"replace taxrate_group_ITEP = 0.027392 if group == "P81-95"replace taxrate_group_ITEP = 0.016966 if group == "P96-99"replace taxrate_group_ITEP = 0.008738 if group == "P99,9-100"graph dot (mean) taxrate_group*, over(group) vertical