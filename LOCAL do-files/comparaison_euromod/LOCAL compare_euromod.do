use "E:\Code\21-03 Datasets V7 (JPubEc Resubmit)\DTA\comparaison_euromod\comparaison_euromod.dta", clearcd "E:\Notes\2021-03 Resubmit JPubEc\Comparaison ITRC Microsim\Réponse\images"// taux d'effort par décileforeach e in 0 1 2 {	twoway line tax_prop* decile_num ///		if decile_num != 11 & tax_prop_ours < 0.4 & etude == `e', ///		by(ccyy_f) yscale(range(0 0.1))			graph export taux_effort_`e'.eps, as(eps) preview (off) replace}// rapport interdécile des taux d'effortforeach e in 0 1 2 {	preserve	keep cname year tax_prop* decile_num etude	drop if decile_num == 11	reshape wide tax_prop tax_prop_ours, i(cname year etude) j(decile_num)	gen ecart_D10_D1 = tax_prop10/tax_prop1	gen ecart_ours_D10_D1 = tax_prop_ours10/tax_prop_ours1	gen ecart_D9_D2 = tax_prop9/tax_prop2	gen ecart_ours2_D9_D2 = tax_prop_ours9/tax_prop_ours2	graph dot (asis) ecart* if etude == `e', over(cname, sort(ecart_D9_D2))	graph export interdecile_effort_`e'.eps, as(eps) preview (off) replace	restore}graph dot (asis) T10_B50 T10_B50_inc5decoster T10_B50_inc5ours ///	if decile_num == 1 & etude == 1, over(ccyy_f) ///	marker(1, msymbol(x)) marker(2, msymbol(x)) marker(3, msymbol(x))	graph export t10_b50.eps, as(eps) preview (off) replace