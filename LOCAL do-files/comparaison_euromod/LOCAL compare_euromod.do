use "E:\Code\21-03 Datasets V7 (JPubEc Resubmit)\DTA\comparaison_euromod\comparaison_euromod.dta", clearcd "E:\Notes\2021-03 Resubmit JPubEc\Comparaison ITRC Microsim\Réponse\images"egen max_year = max(year), by(cname)egen max_etude = max(etude) if year == max_year, by(cname)gen central = year == max_year & etude == max_etudegen D5_tax_prop_t = tax_prop if decile_num == 5gen D5_tax_prop_ours_t = tax_prop_ours if decile_num == 5egen D5_tax_prop = max(D5_tax_prop_t), by(ccyy_f etude)egen D5_tax_prop_ours = max(D5_tax_prop_ours_t), by(ccyy_f etude)drop D5_tax_prop_t D5_tax_prop_ours_tgen rescaled_tax_prop = tax_prop/D5_tax_propgen rescaled_tax_prop_ours = tax_prop_ours/D5_tax_prop_ours// taux d'effort par déciletwoway line rescaled_tax_prop* decile_num ///	if decile_num != 11 & tax_prop_ours < 0.4 & central, ///	by(ccyy_f etude) yscale(range(0 0.1))		graph export taux_effort_central_rescaled.eps, as(eps) preview (off) replace// différences de niveaugraph dot (asis) effective_tax* if central & decile_num == 11, ///	over(etude)  over(ccyy_f, sort(effective_taxrate_ours)) nofill	graph export eff_taxrate_central.eps, as(eps) preview (off) replace	// rapport interdécile des taux d'effortpreservekeep ccyy_f cname year tax_prop* decile_num etude centraldrop if decile_num == 11 | !centralreshape wide tax_prop tax_prop_ours, i(ccyy_f cname year etude) j(decile_num)gen ecart_D10_D1 = tax_prop10/tax_prop1gen ecart_D10_D5 = tax_prop10/tax_prop5gen ecart_D5_D1 = tax_prop5/tax_prop1gen ecart_D9_D2 = tax_prop9/tax_prop2gen ecart_ours_D10_D1 = tax_prop_ours10/tax_prop_ours1gen ecart_ours_D10_D5 = tax_prop_ours10/tax_prop_ours5gen ecart_ours_D5_D1 = tax_prop_ours5/tax_prop_ours1gen ecart_ours2_D9_D2 = tax_prop_ours9/tax_prop_ours2mkmat ecart_D5_D1 ecart_ours_D5_D1 ecart_D10_D5 ecart_ours_D10_D5 ///	ecart_D10_D1 ecart_ours_D10_D1 if central, ///	matrix(interdeciles) rownames(ccyy_f)frmttable using interdeciles.tex, statmat(interdeciles) ///	sdec(2) varlabels tex fragment nocenter replace ///	ctitles("" "D5/D1" "D5/D1 (ITRC)" "D10/D5" "D10/D5 (ITRC)" ///	"D10/D1" "D10/D1 (ITRC)")graph dot (asis) ecart* if central, over(ccyy_f, sort(ecart_D9_D2))graph export interdecile_effort_central.eps, as(eps) preview (off) replacerestoregraph dot (asis) T10_B50 T10_B50_inc5decoster T10_B50_inc5ours ///	if decile_num == 1 & etude == 1, over(ccyy_f) ///	marker(1, msymbol(x)) marker(2, msymbol(x)) marker(3, msymbol(x))	graph export t10_b50.eps, as(eps) preview (off) replace